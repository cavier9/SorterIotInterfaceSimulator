<TextBox x:Class="WFPresentacion.CustomView.CustomTextBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:WFPresentacion.CustomView"
         
              x:Name="customTextBox"
             AcceptsReturn="True"
             Height="371.096" Width="497.203" TextWrapping="Wrap" VerticalAlignment="Top" HorizontalAlignment="Left"
             VerticalScrollBarVisibility="Auto"
         Grid.Column="2" Grid.Row="2" Margin="10,0,0,0"
         Text="{Binding MessageLog, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" IsReadOnly="True"
        
         >
</TextBox>
