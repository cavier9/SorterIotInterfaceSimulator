{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Schema for GetStackerPatternNumber response",
    "type": "object",
    "required": [ "Result", "PatternIndex" ],
    "additionalProperties": false,
    "properties": {
        "Result": { "type": "string", "enum": [ "200 Success", "400 Bad Request", "500 Internal Device Error", "402 Invalid Command", "501 Locked by other system" ] },
        "PatternIndex": { "type": [ "number", "null" ], "maximum": 9 }
    }
}
